function _update60()

end

function _draw()
	cls()
	map(0,0,0,0)
	drawentities(currentlevel)
end

function drawentities(entities)
	for k,v in pairs(entities) do
		if k==1 then
			spr(4,v[1]*8,v[2]*8)
		else
			spr(3,v[1]*8,v[2]*8)
		end
	end
end
-->8
--load

function loadlevel(number)
 if number==0 then
 	currentlevel=parseleveldata(l0)
 end
end

function parseleveldata(entities)
 parsed={}
 for value in all(entities) do
 	add(parsed,{(value%16), flr(value/16)})
 end
 return parsed
end
